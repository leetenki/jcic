<div style="padding-top:30px;" class="col-md-6 col-md-offset-3">
  <%= form_tag('/projects/store_pdf', :multipart => true) do %>
    <%= text_field :project, :id, :class => "form-control", :placeholder => "ID", :value => params[:id] %>
    <%= file_field :project, :pdf, :class => "form-control" %>
    <%= submit_tag "送信", :class => "bnt btn-primary" %>
  <% end %>
</div>
